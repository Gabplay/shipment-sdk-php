{
    "name": "melhorenvio/shipment-sdk",
    "description": "Este pacote é responsável por realizar o cálculo de cotações.",
    "keywords": [
        "melhorenvio",
        "shipment-sdk"
    ],
    "homepage": "https://github.com/melhorenvio/shipment-sdk",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pedro Barros",
            "email": "pedro.barros@melhorenvio.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5"
    },
    "autoload": {
        "psr-4": {
            "MelhorEnvio\\ShipmentSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MelhorEnvio\\ShipmentSdk\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
